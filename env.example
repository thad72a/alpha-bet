# Copy this file to .env.local and fill in your values

# Network Configuration
NEXT_PUBLIC_NETWORK=bittensorTestnet
NEXT_PUBLIC_CHAIN_ID=945
NEXT_PUBLIC_RPC_URL=https://test.chain.opentensor.ai

# Contract Addresses (update after deployment)
NEXT_PUBLIC_BETTING_CONTRACT_ADDRESS=0x...
# Note: TAO is native on Bittensor - no token contract address needed

# Block Explorer
NEXT_PUBLIC_BLOCK_EXPLORER_URL=https://evm.taostats.io

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Deployment Private Key (DO NOT COMMIT THIS - for deployment only)
PRIVATE_KEY=your_private_key_here

# Bittensor API (if using real data)
NEXT_PUBLIC_BITTENSOR_API_URL=https://api.bittensor.com


